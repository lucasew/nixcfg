<?xml version="1.0"?>
<!DOCTYPE fontconfig SYSTEM "urn:fontconfig:fonts.dtd">
<fontconfig>
  <alias>
    <family>serif</family>
    <prefer>
      <family>{{ .Modules.fontconfig.serif }}</family>
    </prefer>
  </alias>
  <alias>
    <family>sans</family>
    <prefer>
      <family>{{ .Modules.fontconfig.sans_serif }}</family>
    </prefer>
  </alias>
  <alias>
    <family>monospace</family>
    <prefer>
      <family>{{ .Modules.fontconfig.monospace }}</family>
    </prefer>
  </alias>
  <alias>
    <family>emoji</family>
    <prefer>
      <family>{{ .Modules.fontconfig.emoji }}</family>
    </prefer>
  </alias>
</fontconfig>
