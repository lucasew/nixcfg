# Auto-generated from template by workspaced
# Hosts configured in settings.toml

{{- range $name, $host := .Hosts }}
{{- if $host.IPs }}
Host {{ $name }}
  HostName {{ index $host.IPs 0 }}
  {{- if $host.Port }}
  Port {{ $host.Port }}
  {{- end }}
  {{- if $host.User }}
  User {{ $host.User }}
  {{- end }}

{{- end }}
{{- end }}
