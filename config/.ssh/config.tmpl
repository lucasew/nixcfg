# Auto-generated from template by workspaced
# Hosts configured in settings.toml

{{- range $name, $host := .Hosts }}
Host {{ $name }}
  HostName {{ $host.TailscaleIP }}
  {{- if $host.Port }}
  Port {{ $host.Port }}
  {{- end }}
  {{- if $host.User }}
  User {{ $host.User }}
  {{- end }}

{{- end }}
